{
    "name": "cdvedia/irsfa",
    "description": "Laravel package for irsfa API",
    "keywords": [
        "cdvedia",
        "irsfa"
    ],
    "homepage": "https://github.com/cdvedia/irsfa",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Cecep Aprilianto",
            "email": "cecepaprilianto@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cdvedia\\Irsfa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdvedia\\Irsfa\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cdvedia\\Irsfa\\IrsfaServiceProvider"
            ],
            "aliases": {
                "Irsfa": "Cdvedia\\Irsfa\\Facades\\Irsfa"
            }
        }
    }
}
